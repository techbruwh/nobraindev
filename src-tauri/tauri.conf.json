{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "NoBrainDev",
  "version": "0.1.1",
  "identifier": "com.techbruwh.nobraindev",
  "build": {
    "devUrl": "http://localhost:1420",
    "frontendDist": "../ui/dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/web/icon-192.png",
      "icons/web/icon-512.png",
      "icons/macos/AppIcon.icns",
      "icons/web/favicon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "NoBrainDev",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
