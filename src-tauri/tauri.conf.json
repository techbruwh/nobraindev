{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "NoBrainDev",
  "version": "1.0.14",
  "identifier": "com.techbruwh.nobraindev",
  "build": {
    "devUrl": "http://localhost:1420",
    "frontendDist": "../ui/dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/web/icon-192.png",
      "icons/web/icon-512.png",
      "icons/macos/AppIcon.icns",
      "icons/web/favicon.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "10.13",
      "bundleName": "NoBrainDev"
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "NoBrainDev",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": "default-src 'self'; connect-src 'self' https://*.clerk.accounts.dev https://*.clerk.com https://clerk.*.lcl.dev; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://*.clerk.accounts.dev https://*.clerk.com; style-src 'self' 'unsafe-inline' https://*.clerk.accounts.dev; img-src 'self' data: https: blob:; font-src 'self' data: https://*.clerk.com; frame-src https://*.clerk.accounts.dev https://*.clerk.com;",
      "devCsp": null
    }
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}